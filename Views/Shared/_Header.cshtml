@using webui.Interfaces
@inject IContentHelperService contentHelperService
@model DefaultModel
<!-- Navbar -->
<nav class="navbar navbar-expand-lg fixed-top navbar-transparent " color-on-scroll="300">
    <div class="container">
        @await contentHelperService.SiteContentBlock("HeaderLogo", Model, Html)
        <!-- Nav bar goes here -->
        @await contentHelperService.SiteContentBlock("HeaderMainMenu", Model, Html)
    </div>
    @*<partial name="_LoginPartial" />*@
    @await Html.PartialAsync("_LoginPartial");
</nav>